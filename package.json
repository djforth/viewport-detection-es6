{
  "name": "viewport-detection-es6",
  "version": "2.1.2",
  "description": "Viewport detaction in es6 ",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "assets": {
    "assets_in": "./src",
    "assets_out": "./lib",
    "karma": {
      "input": "./spec"
    }
  },
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/viewport-detection-es6.git"
  },
  "author": "Adrian Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/viewport-detection-es6/issues"
  },
  "homepage": "https://github.com/djforth/viewport-detection-es6#readme",
  "devDependencies": {
    "@djforth/ap_karma": "^1.0.0",
    "@djforth/ap_utils": "^2.0.13",
    "@djforth/morse-jasmine": "^1.0.10",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.3.0",
    "eslint-config-google": "^0.5.0",
    "estraverse-fb": "^1.3.1",
    "jasmine-ajax": "^3.2.0",
    "lodash": "^4.*.*"
  },
  "peerDependencies": {
    "lodash": "^4.*.*"
  }
}
