{
  "name": "viewport-detection-es6",
  "version": "2.1.4",
  "description": "Viewport detaction in es6 ",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "assets": {
    "assets_in": "./src",
    "assets_out": "./lib",
    "karma": {
      "input": "./spec"
    }
  },
  "scripts": {
    "build": "NODE_ENV=production babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/** --fix",
    "lint:tests": "eslint spec/** --fix",
    "test": "npm run lint && asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "repository": "git+https://github.com/djforth/viewport-detection-es6.git",
  "author": "Adrian Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/viewport-detection-es6/issues"
  },
  "homepage": "https://github.com/djforth/viewport-detection-es6#readme",
  "devDependencies": {
    "@djforth/ap_karma_wp": "^2.0.2",
    "@djforth/ap_utils": "^3.0.4",
    "@djforth/morse-jasmine-wp": "^2.4.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-lodash-modularize": "^0.3.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-rewire-ignore-coverage": "^2.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "eslint": "^2.3.0",
    "eslint-config-google": "^0.5.0",
    "eslint-config-morsedigital": "^1.2.3",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "jasmine-ajax": "^3.2.0",
    "lodash": "^4.17.4",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "lodash": "^4.*.*"
  },
  "dependencies": {}
}
